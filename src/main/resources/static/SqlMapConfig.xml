<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
      "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
   	<typeAlias type ="com.ssafy.model.dto.User" 		alias ="user"/>
   	<!-- <typeAlias type ="com.ssafy.model.dto.FoodPageBean" alias ="pagebean"/> -->
   	<typeAlias type ="com.ssafy.model.dto.Food" 		alias ="food"/>
   	<typeAlias type ="com.ssafy.model.dto.UserFoodBean" alias ="userfood"/>
   	<typeAlias type="com.ssafy.model.dto.QnA"   		alias ="qna"/>
   	<typeAlias type="com.ssafy.model.dto.Comment"   	alias ="comment"/>
   	<typeAlias type="com.ssafy.model.dto.PageBean" 		alias ="pagebean" />
   	<typeAlias type="com.ssafy.model.dto.Notice" 		alias ="notice" />
   	<typeAlias type="com.ssafy.model.dto.NoticeFile" 	alias ="noticefile" />
   	<typeAlias type="com.ssafy.model.dto.Pick"			alias ="pick"/>
  </typeAliases>
    
  <mappers>
     <!--  <mapper url = ""/> 리소스 or URL -->  
     <mapper resource = "static/User_query.xml"/>
     <mapper resource = "static/Food_query.xml"/>
     <mapper resource = "static/Qna_query.xml" />
     <mapper resource = "static/Notice_query.xml" />
     <mapper resource = "static/Pick_query.xml" />
     
  </mappers>
   <!-- query 설정 끝 -->
</configuration>






