<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- MyBatis Query를 위한 Xml에서 resultType, parametertype 으로  
		     사용할  VO 클래스에 대한 별칭 설정 
		  Java AIP에 대한 별칭 설정 정보 : org.apache.ibits.type.TypeAliasRegistry 
		  -->
		<typeAlias type="com.ssafy.hrm.model.dto.Emp"   		alias="emp"/>
		<typeAlias type="com.ssafy.hrm.model.dto.Dept"   		alias="dept"/>
		
	</typeAliases>
	<mappers>
	 <mapper resource="static/emp_query.xml" />
	 <mapper resource="static/dept_query.xml" />
	</mappers>
</configuration>


















